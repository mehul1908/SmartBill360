package com.smartbill360.model;

import com.smartbill360.entity.TaxSlab;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class ProductRegModel {

	private String name;
	
	private String hsnCode;
	
	private Float taxSlab;
	
	private Float rate;
	
	public ProductRegModel(String name, String hsnCode, Float taxSlab, Float rate) {
		super();
		this.name = name;
		this.hsnCode = hsnCode;
		this.taxSlab= taxSlab;
		this.rate = rate;
	}
	
	
	
}
